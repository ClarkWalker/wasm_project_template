{
    "name": "wasm_project_template",
    "version": "1.0.0",
    "description": "create new WASM project",
    "main": "app.js",
    "directories": {
        "doc": "docs",
        "test": "test"
    },
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.12",
        "filewatcher": "^3.0.1",
        "chai": "^4.3.4",
        "mocha": "^9.0.3",
        "wat-wasm": "^1.0.37"
    },
    "scripts": {
        "test": "./node_modules/mocha/bin/mocha",
        "start": "node server.js",
        "dev": "node scripts/compile.js"
    },
    "nodemonConfig": {
        "ignore": [
            "bin",
            "src/main.cpp",
            "src/lib.cpp"
        ],
        "watch": [
            ".",
            "run.sh",
            "index.html",
            "scripts/compile.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theheartbeast/wasm_project_template.git"
    },
    "keywords": [
        "wasm",
        "js",
        "cpp",
        "c++",
        "project",
        "template"
    ],
    "author": "Clark Walker (theheartbeast)",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/theheartbeast/wasm_project_template/issues"
    },
    "homepage": "https://github.com/theheartbeast/wasm_project_template#readme"
}
